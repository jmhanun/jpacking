	<parameter name="pFechaDesde" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="pFechaHasta" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     detmovimientosstock.`descripcion` AS detmovimientosstock_descripcion,
     detmovimientosstock.`cantidad` AS detmovimientosstock_cantidad,
     detmovimientosstock.`fechaMovimiento` AS detmovimientosstock_fechaMovimiento,
     articulos.`codigo` AS articulos_codigo,
     unidadesmedida.`abreviatura` AS unidadesmedida_abreviatura,
     case
       when remitos.`idRemito` is not null = 1 THEN 'Remito'
       when notasdebito.`idNotaDebito` is not null = 1 THEN 'Nota de Débito'
       when remitosingreso.`idRtoIngreso` is not null = 1 THEN 'Remito de Ingreso'
       when notascredito.`idNotaCredito` is not null = 1 THEN 'Nota de Crédito'
       when ordenesproduccion.`idOrdenProduccion` is not null = 1 THEN 'Orden de Producción'
       when ordenesdeposito.`idOrdenDeposito` is not null = 1 THEN 'Orden de Depósito'
       end tipocomprobante,
     case
       when remitos.`idRemito` is not null = 1 THEN remitos.`nroRemito`
       when notasdebito.`idNotaDebito` is not null = 1 THEN notasdebito.`nroNotaDebito`
       when remitosingreso.`idRtoIngreso` is not null = 1 THEN remitosingreso.`nroRemito`
       when notascredito.`idNotaCredito` is not null = 1 THEN notascredito.`nroNotaCredito`
       when ordenesproduccion.`idOrdenProduccion` is not null = 1 THEN ordenesproduccion.`nroOrdenProduccion`
       when ordenesdeposito.`idOrdenDeposito` is not null = 1 THEN ordenesdeposito.`idOrdenDeposito`
       end nrocomprobante
FROM
     `articulos` articulos INNER JOIN `detmovimientosstock` detmovimientosstock ON articulos.`idArticulo` = detmovimientosstock.`idArticulo`
     INNER JOIN `unidadesmedida` unidadesmedida ON articulos.`idUnidMedida` = unidadesmedida.`idUnidMedida`
     AND unidadesmedida.`idUnidMedida` = detmovimientosstock.`idUnidMedida`
     INNER JOIN `notascredito` notascredito ON detmovimientosstock.`idNotaCredito` = notascredito.`idNotaCredito`
     INNER JOIN `notasdebito` notasdebito ON detmovimientosstock.`idNotaDebito` = notasdebito.`idNotaDebito`
     INNER JOIN `ordenesdeposito` ordenesdeposito ON detmovimientosstock.`idOrdenDeposito` = ordenesdeposito.`idOrdenDeposito`
     INNER JOIN `ordenesproduccion` ordenesproduccion ON detmovimientosstock.`idOrdenProduccion` = ordenesproduccion.`idOrdenProduccion`
     INNER JOIN `remitos` remitos ON detmovimientosstock.`idRemito` = remitos.`idRemito`
     INNER JOIN `remitosingreso` remitosingreso ON detmovimientosstock.`idRtoIngreso` = remitosingreso.`idRtoIngreso`
     AND remitos.`idRemito` = ordenesproduccion.`idRemito`
WHERE detmovimientosstock.`fechaMovimiento` between $P{pFechaDesde} and $P{pFechaHasta}]]>
	</queryString>
