	<queryString>
		<![CDATA[SELECT
     estados.`descripcion` AS estados_descripcion,
     proveedores.`idProveedor` AS proveedores_idProveedor,
     proveedores.`observaciones` AS proveedores_observaciones,
     proveedores.`nombres` AS proveedores_nombres,
     proveedores.`apellido` AS proveedores_apellidos,
     proveedores.`mails` AS proveedores_mails,
     proveedores.`telefonos` AS proveedores_telefonos,
     proveedores.`fechaAlta` AS proveedores_fechaAlta,
     proveedores.`cuit` AS proveedores_cuit
FROM
     `estados` estados INNER JOIN `proveedores` proveedores ON estados.`idEstado` = proveedores.`idEstado`
WHERE proveedores.`idEstado` = 10
  AND proveedores.`idProveedor`=IFNULL($P{pproveedor}, proveedores.`idProveedor`)
ORDER BY proveedores.`nombres`, proveedores.`idProveedor`]]>
	</queryString>
